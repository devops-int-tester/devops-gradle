pipeline {
   agent any
   stages {
       stage("build") {
                steps {
                    snDevOpsStep()
                    echo "Building" 
                    sh "./gradlew clean build"
                }
       }
      stage("test") {
           steps {
               snDevOpsStep()
               echo "Testing"
               sh "./gradlew clean test"
           }
        }
    
      stage("deploy") {
             steps{
                  snDevOpsStep()
                  echo "deploying"
              }
      }      
      
  }
}
